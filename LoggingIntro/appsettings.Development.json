{
    "Logging":{
        "LogLevel":{
            "Default":"Information",
            "Microsoft.AspNetCore":"Information"
        },
        "Console":{
            "LogLevel": {
                "Default":"Information",
                "Microsoft.AspNetCore":"Information"
            },
            "FormatterName": "json",
            "FormatterOptions": {
                "SingleLine":true,
                "IncludeScopes":true,
                "TimestampFormat": "HH:mmss",
                "UseUtcTimestamp": true,
                "JsonWriterOptions":{
                    "Intended":true
                }
            }
        }
    }
}